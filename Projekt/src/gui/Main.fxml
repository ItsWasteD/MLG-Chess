<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.MainController">
   <children>
      <Button layoutX="225.0" layoutY="63.0" mnemonicParsing="false" prefHeight="37.0" prefWidth="151.0" text="Play MLG Chess">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button layoutX="225.0" layoutY="132.0" mnemonicParsing="false" prefHeight="37.0" prefWidth="151.0" text="Customize" onAction="#changeCustomize">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button cancelButton="true" layoutX="225.0" layoutY="271.0" mnemonicParsing="false" onAction="#quit" prefHeight="37.0" prefWidth="151.0" text="Quit" />
      <Button fx:id="btnInfo" layoutX="24.0" layoutY="353.0" mnemonicParsing="false" onAction="#changeInfo" prefHeight="25.0" prefWidth="27.0" styleClass="roundButton" stylesheets="@style.css" text="?" />
      <ImageView fx:id="imgView" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
